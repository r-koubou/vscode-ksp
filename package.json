{
    "name": "ksp-v1-preview",
    "displayName": "KONTAKT Script Processor (KSP) [v1.0 Pre-Release]",
    "description": "Language support for NI KONTAKT(TM) Script Processor (KSP)",
    "version": "0.9.0",
    "preview": true,
    "publisher": "rkoubou",
    "homepage": "https://r-koubou.net",
    "license": "MIT",
    "keywords": [
        "KONTAKT",
        "KSP",
        "Native Instruments",
        "Computer Music",
        "DTM"
    ],
    "categories": [
        "Programming Languages",
        "Linters"
    ],
    "galleryBanner": {
        "color": "#3A3D40",
        "theme": "dark"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/r-koubou/vscode-ksp.git"
    },
    "bugs": {
        "url": "https://github.com/r-koubou/vscode-ksp/issues"
    },
    "icon": "resources/icon.png",
    "main": "./out/extension.js",
    "contributes": {
        "languages": [
            {
                "id": "ksp",
                "configuration": "./language-configuration.json",
                "aliases": [
                    "KONTAKT Script (KSP) [Pre-Release]"
                ],
                "extensions": [
                    ".ksp"
                ]
            }
        ],
        "grammars": [
            {
                "language": "ksp",
                "scopeName": "source.kontakt.ksp.rkoubou",
                "path": "./syntaxes/ksp.json"
            }
        ]
    },
    "engines": {
        "vscode": "^1.96.0"
    },
    "dependencies": {
        "vscode-languageclient": "^9.0.1"
    },
    "devDependencies": {
        "@types/mocha": "^10.0.10",
        "@types/node": "20.x",
        "@types/vscode": "^1.96.0",
        "@vscode/vsce": "^3.2.2",
        "@typescript-eslint/eslint-plugin": "^8.17.0",
        "@typescript-eslint/parser": "^8.17.0",
        "eslint": "^9.16.0",
        "typescript": "^5.7.2"
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src",
        "test": "vscode-test"
    }
}