{
    "name": "ksp-v1-preview",
    "displayName": "KONTAKT Script Processor (KSP) [v1.0 Pre-Release]",
    "description": "Deprecated. Plaease read the README.md",
    "version": "0.9.3",
    "preview": true,
    "publisher": "rkoubou",
    "homepage": "https://r-koubou.net",
    "license": "MIT",
    "keywords": [
        "KONTAKT",
        "KSP",
        "Native Instruments",
        "Computer Music",
        "DTM"
    ],
    "categories": [
        "Programming Languages",
        "Linters"
    ],
    "galleryBanner": {
        "color": "#3A3D40",
        "theme": "dark"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/r-koubou/vscode-ksp.git"
    },
    "bugs": {
        "url": "https://github.com/r-koubou/vscode-ksp/issues"
    },
    "icon": "resources/icon.png",
    "main": "./dist/extension.js",
    "files": [
        "dist/**/*.js",
        "language_server/**",
        "syntaxes/**",
        "resources/**",
        "language-configuration.json",
        "package.json",
        "LICENSE",
        "README.md",
        "NOTICE.md",
        "CHANGELOG.md"
    ],
    "contributes": {
        "languages": [
            {
                "id": "ksp",
                "configuration": "./language-configuration.json",
                "aliases": [
                    "KONTAKT Script (KSP) [Pre-Release]"
                ],
                "extensions": [
                    ".ksp"
                ]
            }
        ],
        "grammars": [
            {
                "language": "ksp",
                "scopeName": "source.kontakt.ksp.rkoubou",
                "path": "./syntaxes/ksp.json"
            }
        ],
        "commands": [
            {
                "command": "ksp.lsp.restart",
                "category": "KSP",
                "title": "Restart Language Server"
            },
            {
                "command": "ksp.obfuscate",
                "category": "KSP",
                "title": "Obfuscate Code"
            }
        ],
        "properties": {}
    },
    "engines": {
        "vscode": "^1.108.0"
    },
    "dependencies": {
        "vscode-languageclient": "^9.0.1"
    },
    "extensionDependencies": [
        "ms-dotnettools.vscode-dotnet-runtime"
    ],
    "devDependencies": {
        "@types/mocha": "^10.0.10",
        "@types/node": "^22.19.7",
        "@types/vscode": "^1.108.1",
        "@vscode/vsce": "^3.7.1",
        "@typescript-eslint/eslint-plugin": "^8.54.0",
        "@typescript-eslint/parser": "^8.54.0",
        "esbuild": "^0.27.2",
        "eslint": "^9.39.2",
        "typescript": "^5.8.3"
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "npm run check-types && npm run lint && node esbuild.js",
        "watch": "npm-run-all -p watch:*",
        "watch:esbuild": "node esbuild.js --watch",
        "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
        "package": "npm run check-types && npm run lint && node esbuild.js --production",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "npm run compile-tests && npm run compile && npm run lint",
        "check-types": "tsc --noEmit",
        "lint": "eslint src",
        "test": "vscode-test"
    }
}